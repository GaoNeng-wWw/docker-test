{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "moduleResolution": "node",
    "strict": false,
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "lib": [
      "es2020",
      "dom"
    ],
    "skipLibCheck": true,
    "types": [
      "./src/env.d.ts"
    ]
  },
  "include": [
    "src/**/*",
    "src/**/*.vue",
    "src/env.d.ts"
  ],
  "exclude": [
    "node_modules"
  ],
}